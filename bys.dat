    $(function () {
        /*回复的内容*/
        var temp = '\<dd class="res_list">\
                <div class="res_c_list clearfix">\
                        <div class="res_c_l"><a href="{%href%}" target="_blank"><img src="{%header%}" alt="{%username%}"></a></div>\
             <div class="res_c_r">\
                <div class="res_c_t clearfix">\
